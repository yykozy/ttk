		<form id="ttk-form">
                    <div class="form-inline">
                        <div class="form-artist">
                            <div>
                                <label class="">アーティスト名</label>
                            </div>
                            <div>
                                <input type="text" class="form-control" id="artist" placeholder="アーティスト名または公演名を入力" value="<%=data.request.artist %>">
                            </div>
                        </div>
                        <div class="form-place">
                            <label class="">場所</label>
                            <input type="text" class="form-control" id="place" placeholder="都道府県、会場名を入力" value="<%=data.request.place %>">
                        </div>
                        <div class="form-date">
                            <label class="">日付</label>
                            <input type="text" class="form-control" id="date" placeholder="日付を選択" value="<%=data.request.date %>">
                        </div>
                        <div class="form-search">
                            <label class="">　</label>
                            <button id="search" class="btn form-control btn-search"><span class="glyphicon glyphicon-search"></span><span class="search-btn-str">検索</span></button>
                        </div>
                        <input type="hidden" id="keyword" value="<%=data.request.keyword %>">
                    </div>
		</form>
                </div>
            </div>
