                <div id="messageView">
                    <ol class="stream-items">
                        <% if(!data.tweets.statuses || data.tweets.statuses.length==0){ %>
                        <li>該当するツイートが見つかりませんでした。</li>
                        <% }else{ %>
                        <% for(var i=0;i<data.tweets.statuses.length;i++){
                            var tweet = data.tweets.statuses[i]; %>
                        <li class="stream-item">
                            <div class="tweet">
                                <div class="content">
                                    <div class="stream-item-header">
                                        <a class="tweetlink" rel="nofollow" href="https://twitter.com/<%=tweet.screen_name %>">
                                            <img class="avatar" src="<%=tweet.user.profile_image_url.replace("http:","https:") %>">
                                            <strong class="fullname"><%=tweet.user.name %></strong>
                                            <span class="screen-name">@<%=tweet.user.screen_name %></span>
                                        </a>
                                        <a class="timeago" rel="nofollow" title="<%=tweet.timeago %>" href="<%=tweet.link%>"><%=tweet.timeago %></a>
                                    </div>
                                    <p class="tweet-text"><%-tweet.text_linked %></p>
                                    <div class="tweet-action">
                                        <a href="https://twitter.com/intent/tweet?in_reply_to=<%=tweet.id_str %>" rel="nofollow" target="_blank"><span class="glyphicon glyphicon-share-alt"></span></a>
                                        <a href="https://twitter.com/intent/retweet?tweet_id=<%=tweet.id_str %>" rel="nofollow" target="_blank"><span class="glyphicon glyphicon-retweet"></span></a>
                                        <a href="https://twitter.com/intent/favorite?tweet_id=<%=tweet.id_str %>" rel="nofollow" target="_blank"><span class="glyphicon glyphicon-heart"></span></a>
                                    </div>
<!--
                                <div class="extraction">
